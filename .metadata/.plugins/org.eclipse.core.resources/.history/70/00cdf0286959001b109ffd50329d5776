package com.ssafy.ws01.step3;

public class DigitTest2 {
	public static void main(String[] args) {
		
	
	int cnt = 1;
	
	for(int i = 0 ; i< 5; i+=2) 
		{
		
		
			for(int k=5-i;k<5;k++) {
				System.out.print("  ");
			}
			for(int j=5-i; j>0; j--) {
				System.out.print(cnt++ +"   ");
			}
			

		System.out.println();
		
		}
	
	for(int i = 0;i<2;i++) {
		for(int k=-0;k<=i;k++) {
			System.out.print("   ");
		}
		for(int j=3;j<=5;j++) {
			System.out.print(cnt++ +"   ");
		}
	System.out.println();
	}
	
	
}
	}
