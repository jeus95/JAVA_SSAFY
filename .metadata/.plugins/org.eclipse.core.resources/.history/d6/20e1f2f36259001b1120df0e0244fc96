package com.ssafy.ws01.step3;

public class DigitTest1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int sum =1;
		for(int i = 0 ; i< 5; i++) {
			for(int k=5-i;k<0;k--) {
				System.out.print(" ");
			}
			for(int j=5-i; j<0; j--) {
				System.out.print(sum++);
			}
		System.out.println();
		cnt++;
	 }
	}
}
