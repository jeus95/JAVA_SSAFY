package com.ssafy.ws01.step3;

public class DigitTest1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int cnt = 0;
		
		for(int i = 0 ; i< 5; i++) {
			for(int k=5-i;k>5;k++) {
				System.out.print(" ");
			}
			for(int j=5-i; j>0; j--) {
				System.out.println(cnt++);
			}
		System.out.println();

	 }
	}
}